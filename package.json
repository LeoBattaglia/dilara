{
    "name":"dilara",
    "version":"0.0.1",
    "description":"NOT USABLE: Simple and lightweight TypeScript-Framework to host a lot of small- or medium-sized web-projects.",
    "main":"dilara.js",
    "scripts":{},
    "keywords":[
        "typescript",
        "javascript",
        "node.js",
        "framework",
        "hosting",
        "mysql",
        "webserver"
    ],
    "author":"Leo Battaglia",
    "license":"ISC",
    "dependencies":{
        "@types/node":"^14.14.6",
        "powerprompt":"^1.1.3",
        "typescript":"^4.0.5"
    },
    "http": true,
    "port_http": 80,
    "port_https": 8000,
    "host": "localhost",
    "timeout": 1000,
    "cmd": {
        "cert": "cert",
        "exit": "exit"
    },
    "cmds": [
        {
            "name": "cert",
            "description": "Create new SSL-Certification"
        },
        {
            "name": "exit",
            "description": "Close Application"
        }
    ],
    "cmd_cli": {
        "start": "start"
    }
}
